<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.MainController">
   <children>
      <AnchorPane prefHeight="800.0" prefWidth="800.0">
         <children>
            <Button fx:id="loginButton" layoutX="319.0" layoutY="610.0" mnemonicParsing="false" onAction="#login" text="Login" />
            <Button fx:id="registerButton" layoutX="309.0" layoutY="643.0" mnemonicParsing="false" onAction="#register" text="Register" />
            <TextField fx:id="username" layoutX="260.0" layoutY="511.0" promptText="Username" />
            <PasswordField fx:id="password" layoutX="260.0" layoutY="548.0" promptText="Password" />
            <Label fx:id="labelStatus" layoutX="284.0" layoutY="588.0" prefHeight="15.0" prefWidth="125.0" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
